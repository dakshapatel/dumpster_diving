

<%=form_for (@dumpster) do |f|%>

        <div class="form-group">
                <%= f.label :name %><br>
                <%= f.text_field :name, class: "form-control" %></div>
    
        <div class="form-group">
                <%= f.label :address %><br>
                <%= f.text_field :address, class: "form-control" %></div>

        <div class="form-group">
                <%= f.label :city %><br>
                <%= f.text_field :city, class: "form-control" %></div>

        <div class="form-group">
                <%= f.label :state %><br>
                <%= f.text_field :state, class: "form-control" %></div>
    
        <div class="form-group">
                <%= f.label :zipcode %><br>
                <%= f.text_field :zipcode, class: "form-control" %></div>
        <%= f.submit %>


<%end%>

